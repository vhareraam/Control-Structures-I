#include<iostream>
using namespace std;
int main()
{
     int n,x,y,p,q,count;
     cin>>n;
     for (count = 0; n > 0; count++){
        n = n / 10;
    }
         p=n/100;
         q=n%100;
         x=q/10;
         y=q%10;
    
    if(n==((p^count)+(x^count)+(y^count))){
         cout<<"Armstrong number";
         }
    else{
       cout<<"Not an Armstrong Number";
        
    }
    
        
    
}
