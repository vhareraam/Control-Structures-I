#include<iostream>
using namespace std;
int main()
{
     int n,x,y,m,o,p,q,t;
     cin>>n;
         m=n/1000;
         o=n%1000;
         p=o/100;
         q=o%100;
         x=q/10;
         y=q%10;
         
    t=(m+p+x+y);
    if(n%t==0){
         cout<<"Harshard number";
         }
    else{
        cout<<"Not Harshard number";
    }
}
